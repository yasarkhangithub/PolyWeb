PREFIX gs: <http://sels.insight.org/cancer-genomics/schema/>

SELECT ?cnv ?start ?end ?mut ?chr

WHERE { 
	?cnv gs:chr ?chr .
	?cnv gs:gene <http://sels.insight.org/cancer-genomics/gene/BRCA2> .
	?cnv gs:start ?start .
	?cnv gs:end ?end .
	?cnv gs:mutation_type ?mut .
	
	Filter (?mut = "gain" || ?mut = "loss")
	
}